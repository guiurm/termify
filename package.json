{
    "name": "cli-frw",
    "version": "0.0.0",
    "main": "src/main.ts",
    "author": "guiurm",
    "scripts": {
        "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
        "dev2": "ts-node -r tsconfig-paths/register src/main.ts myapp -url qqq.com",
        "dev": "ts-node -r tsconfig-paths/register src/main.ts ci -p 80 --env=./.env www.doamin.com some_token",
        "dev3": "ts-node -r tsconfig-paths/register src/main.ts myapp -f --verbose --output=file.txt --foo=bar somePositionalArg -- someOtherArg",
        "format": "prettier --ignore-path .prettierignore --write src/"
    },
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@types/node": "^22.9.0",
        "eslint": "^9.14.0",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    }
}
